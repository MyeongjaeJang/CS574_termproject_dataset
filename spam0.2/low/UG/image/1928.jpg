        $(document).ready(function(){
            $('.levitator').animate({ height:418 }, function(){
                $("#close").fadeIn();
                $(".collapsed-add").fadeIn();
                $("#toggle1").fadeOut("fast");
            });
            function expand(){

                $("#toggle1").fadeOut("fast");
                //$(".expandable-intro").fadeOut("fast");
                $('.levitator').animate({ height:418 }, function(){
                    $("#close").fadeIn();
                    $(".collapsed-add").fadeIn();
                });
            }
            function contract(){
                $("#close").fadeOut();
                $(".collapsed-add").fadeOut();
                //$("#toggle1").fadeIn("fast");
                $('.levitator').delay(500).animate({ height:65 },function(){
                    $("#toggle1").fadeIn("fast");
                    //$(".expandable-intro").fadeIn("slow");
                });}
            $("#toggle1").click(function(){expand();});
            $("#close").click(function(){

                contract();
            });
            setTimeout(function() {
                // Do something after 2 seconds
                contract();
            }, 5000);
        });
    </script>
